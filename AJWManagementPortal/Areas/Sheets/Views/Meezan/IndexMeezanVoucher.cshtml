@model IEnumerable<AJWManagementPortal.Models.MeezanBankIEVoucher>

@{
    ViewData["Title"] = "DailyTransactionReportList";
    Layout = "~/Views/Shared/_LayoutAccount.cshtml";
}
<table class="tabled table table-bordered">
    <tr >
        <th class="bg-white text-dark" scope="col" colspan="10">
            <a type="button" class="btn btn-black" asp-area="Account" asp-controller="Account" asp-action="Sheets">Back To Sheet Page </a>

            <a type="button" class="btn btn-info" asp-action="CreateMeezanVoucher">+ Create New Sheet </a>
        </th>
    </tr>
    <tr>
        <th class="bg-dark text-white" scope="col" colspan="10"><img src="~/Image/white.png" alt="Image" width="100" height="100" /><br />اے جے ڈبلیو ایگرو انڈسٹری</th>
    </tr>
    <tr>
        <th class="bg-dark text-white" scope="col" colspan="10">ڈیلی میزان بینک انکم خرچ رپورٹ لسٹ اکاؤنٹ آفس</th>
    </tr>
</table>
<table class="tabled table table-bordered" id="DMVR">
    <thead>
        <tr class="bg-dark text-white">
            <th class="text-left" colspan="10" scope="col"> <input type="datetime-local" id="birthdaytime" name="birthdaytime">  : تاریخ </th>
        </tr>
        <tr>
            <th class="text-center bg-dark text-white" scope="col">ڈسپلے لسٹ</th>
            <th class="text-center bg-dark text-white" scope="col">تاریخ</th>
            <th class="text-center bg-dark text-white" scope="col">ٹائٹل</th>
            <th class="text-center bg-dark text-white" scope="col">نمبر شمار</th>
        </tr>
    </thead>
    <tbody>     
        @foreach (var item in Model)
        {
            <tr>
                <td scope="row" style="width:150px">

                    <div class="btn-group" role="group">
                        <a class="btn btn-success text-white" asp-action="CreateMeezanVoucher" asp-route-Id="@item.dateTime.ToString("MM-dd-yyyy")" asp-route-hide="true">
                            <i class="fas fa-list-alt">Edit</i>
                        </a>

                    </div>

                </td>
                <td class="text-center bg-dark text-white" scope="col">@item.dateTime </td>
                <td class="text-center bg-dark text-white" scope="col">ڈیلی میزان بینک انکم خرچ رپورٹ  </td>
                <td class="text-center bg-dark text-white" scope="col">@item.Id</td>
            </tr>
        }

    </tbody>
</table>

<style>
    .tabled {
        position: center;
        align-content: center;
        text-align: center;
        /*margin-left: -200px;*/
    }

    .signt {
        position: center;
        align-content: center;
        text-align: center;
        /*  margin-left: -120px;*/
    }
</style>

@section scripts{
    <script>
        $(document).ready(function () {
            $('#DMVR').DataTable();
        });
    </script>

}