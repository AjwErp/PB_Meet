                            </th>
                            <th colspan="3">
                                @Localizer["Cash Description"]
                            </th>

                        </tr>
                        <tr>

                            <th colspan="7" class="text-center">
                                <input class="form-control" placeholder="@Localizer["Enter Purpose of Cash..."]" />
                                <span class="text-danger"></span>
                            </th>
                            <th colspan="3">
                                @Localizer["Purpose"]
                            </th>

                        </tr>
                        <tr>
                            <th scope="col" colspan="3">@Localizer["Remaning Balance"]</th>
                            <th scope="col" colspan="2">@Localizer["Income Tax"] </th>
                            <th scope="col" colspan="2">@Localizer["Check Cash"]</th>
                            <th scope="col" colspan="3">@Localizer["Current Bank Balance"]</th>
                        </tr>
                        <tr>
                            <th scope="row" colspan="3">
                            </th>
                            <th scope="row" colspan="2">0</th>
                            <th scope="row" colspan="2"></th>
                            <th scope="row" colspan="3">
                            </th>
                        </tr>
                        <tr>
                            <th colspan="7" class="text-right">
                                <input class="form-control" placeholder="@Localizer["Enter Description for Cash Consumer..."]" />
                                <span class="text-danger"></span>
                            </th>
                            <th colspan="3">
                                @Localizer["Description"]
                            </th>
                        </tr>
                        <tr>
                        </tr>
                        <tr>
                            <th colspan="10">@Localizer["Select Supporting Documents"]</th>
                        </tr>
                        <tr>
                            <th colspan="10"> <input type="file" id="myFile" name="filename"></th>
                        </tr>
                        <tr>
                            <td colspan="10">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                                <img src="~/Image/yok.jpg" class="img-rounded " alt="Cinque Terre" width="200" height="50">
                            </td>
                        </tr>
                        <tr>
                            <td colspan="10"></td>
                        </tr>
                        <tr>
                            <th colspan="10" scope="col">@Localizer["Signature"]</th>
                        </tr>
                        <tr>
                            <th scope="row" colspan="3">@Localizer["Account Manager"]</th>
                            <th colspan="4">@Localizer["D.G.M"]</th>
                            <th colspan="3">@Localizer["G.M"]</th>
                        </tr>
                        <tr>
                            <td colspan="3">
                                @Localizer["Signature"]:
                                <input type="text" id="paidAmount" class="form-control" />
                                @Localizer["Remarks"]:
                                <input type="text" id="paidAmount" class="form-control" />
                            </td>
                            <td colspan="4">
                                @Localizer["Signature"]:
                                <input type="text" id="paidAmount" class="form-control" />
                                @Localizer["Remarks"]:
                                <input type="text" id="paidAmount" class="form-control" />
                            </td>
                            <td colspan="3">
                                @Localizer["Signature"]:
                                <input type="text" id="paidAmount" class="form-control" />
                                @Localizer["Remarks"]:
                                <input type="text" id="paidAmount" class="form-control" />
                            </td>

                        </tr>
                        <tr>
                            <td scope="row" colspan="10" class="actionbtn">
                                <div class="btn-group">
                                    <button class="btn btn-primary btn-sm dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        @Localizer["Action"]
                                    </button>
                                    <div class="dropdown-menu">
                                        <a class="dropdown-item" asp-action="" asp-controller="" asp-area="Account"><i class="fas fa-save"></i> @Localizer["Save File"]</a>
                                        <a class="dropdown-item" asp-action="" asp-controller="" asp-area="Account"><i class="fas fa-trash-restore"></i> @Localizer["Clear Sheet"]</a>
                                    </div>
                                </div>
                            </td>
                        </tr>

                    </tbody>
                </table>
            </form>
        </div>
    </div>
</body>
<!-- back to top button-->
<a id="button"></a>
