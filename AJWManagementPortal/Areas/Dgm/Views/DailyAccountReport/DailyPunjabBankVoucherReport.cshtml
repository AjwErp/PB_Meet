                        <tr>
                            <th colspan="10"><img src="~/Image/1.png" alt="Image" width="100" height="100" /></th>
                        </tr>
                        <tr>
                            <th class="titleAccountofficeReport" colspan="10">@Localizer["Daily Punjab Bank Income/Expence Voucher Report-Account Office"]</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th class="text-left" colspan="5" scope="col">
                                @Localizer["Check No"]
                                : C-001
                                <span class="text-danger"></span>
                            </th>
                            <th class="text-center" colspan="5" scope="col">
                                <span class="">  @Localizer["Date"]</span>
                                : 04/05/2021
                                <span class="text-danger"></span>

                            </th>

                        </tr>
                        <tr>


                            <th colspan="7" class="text-right">
                                <input class="form-control" placeholder="  @Localizer["Enter Cash Description ..."]" />
